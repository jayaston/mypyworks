from numpy import array

# split a univariate sequence into samples
def split_sequence(sequence, n_steps_in, n_steps_out):
	X, y =3D list(), list()
	for i in range(len(sequence)):
		# find the end of this pattern
		end_ix =3D i + n_steps_in
		out_end_ix =3D end_ix + n_steps_out
		# check if we are beyond the sequence
		if out_end_ix &gt; len(sequence):
			break
		# gather input and output parts of the pattern
		seq_x, seq_y =3D sequence[i:end_ix], sequence[end_ix:out_end_ix]
		X.append(seq_x)
		y.append(seq_y)
	return array(X), array(y)

# define input sequence
raw_seq =3D [10, 20, 30, 40, 50, 60, 70, 80, 90]
# choose a number of time steps
n_steps_in, n_steps_out =3D 3, 2
# split into samples
X, y =3D split_sequence(raw_seq, n_steps_in, n_steps_out)
# summarize the data
for i in range(len(X)):
	print(X[i], y[i])</textarea></div>
