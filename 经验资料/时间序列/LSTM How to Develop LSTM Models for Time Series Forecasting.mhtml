model.add(LSTM(200, activation=3D'relu', return_sequences=3DTrue))
model.add(TimeDistributed(Dense(n_features)))
model.compile(optimizer=3D'adam', loss=3D'mse')
# fit model
model.fit(X, y, epochs=3D300, verbose=3D0)
# demonstrate prediction
x_input =3D array([[60, 65, 125], [70, 75, 145], [80, 85, 165]])
