portant; line-height: 15px !important;"># univariate cnn lstm example
from numpy import array
from keras.models import Sequential
from keras.layers import LSTM
from keras.layers import Dense
from keras.layers import Flatten
from keras.layers import TimeDistributed
from keras.layers.convolutional import Conv1D
from keras.layers.convolutional import MaxPooling1D

# split a univariate sequence into samples
def split_sequence(sequence, n_steps):
	X, y =3D list(), list()
	for i in range(len(sequence)):
		# find the end of this pattern
		end_ix =3D i + n_steps
		# check if we are beyond the sequence
		if end_ix &gt; len(sequence)-1:
			break
		# gather input and output parts of the pattern
		seq_x, seq_y =3D sequence[i:end_ix], sequence[end_ix]
		X.append(seq_x)
		y.append(seq_y)
	return array(X), array(y)

# define input sequence
raw_seq =3D [10, 20, 30, 40, 50, 60, 70, 80, 90]
# choose a number of time steps
n_steps =3D 4
# split into samples
X, y =3D split_sequence(raw_seq, n_steps)
# reshape from [samples, timesteps] into [samples, subsequences, timesteps,=
 features]
n_features =3D 1
n_seq =3D 2
n_steps =3D 2
X =3D X.reshape((X.shape[0], n_seq, n_steps, n_features))
# define model
model =3D Sequential()
model.add(TimeDistributed(Conv1D(filters=3D64, kernel_size=3D1, activation=
=3D'relu'), input_shape=3D(None, n_steps, n_features)))
model.add(TimeDistributed(MaxPooling1D(pool_size=3D2)))
model.add(TimeDistributed(Flatten()))
model.add(LSTM(50, activation=3D'relu'))
model.add(Dense(1))
model.compile(optimizer=3D'adam', loss=3D'mse')
# fit model
model.fit(X, y, epochs=3D500, verbose=3D0)
# demonstrate prediction
x_input =3D array([60, 70, 80, 90])
x_input =3D x_input.reshape((1, n_seq, n_steps, n_features))
yhat =3D model.predict(x_input, verbose=3D0)
print(yhat)</textarea></div>
