portant; line-height: 15px !important;"># univariate stacked lstm example
from numpy import array
from keras.models import Sequential
from keras.layers import LSTM
from keras.layers import Dense

# split a univariate sequence
def split_sequence(sequence, n_steps):
	X, y =3D list(), list()
	for i in range(len(sequence)):
		# find the end of this pattern
		end_ix =3D i + n_steps
		# check if we are beyond the sequence
		if end_ix &gt; len(sequence)-1:
			break
		# gather input and output parts of the pattern
		seq_x, seq_y =3D sequence[i:end_ix], sequence[end_ix]
		X.append(seq_x)
		y.append(seq_y)
	return array(X), array(y)

# define input sequence
raw_seq =3D [10, 20, 30, 40, 50, 60, 70, 80, 90]
# choose a number of time steps
n_steps =3D 3
# split into samples
X, y =3D split_sequence(raw_seq, n_steps)
# reshape from [samples, timesteps] into [samples, timesteps, features]
n_features =3D 1
X =3D X.reshape((X.shape[0], X.shape[1], n_features))
# define model
model =3D Sequential()
model.add(LSTM(50, activation=3D'relu', return_sequences=3DTrue, input_shap=
e=3D(n_steps, n_features)))
model.add(LSTM(50, activation=3D'relu'))
model.add(Dense(1))
model.compile(optimizer=3D'adam', loss=3D'mse')
# fit model
model.fit(X, y, epochs=3D200, verbose=3D0)
# demonstrate prediction
x_input =3D array([70, 80, 90])
x_input =3D x_input.reshape((1, n_steps, n_features))
yhat =3D model.predict(x_input, verbose=3D0)
print(yhat)</textarea></div>
